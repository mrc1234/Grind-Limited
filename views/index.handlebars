                    <div class="form-group">
                      <input type="password" value="" placeholder="Company Password" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-3">
                    <button type="button" class="btn btn-primary btn-block">Sign up</button>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
